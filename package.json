{
  "name": "shonen-jump-api",
  "version": "1.1.0",
  "description": "An API showing data about English releases from Weekly Shonen Jump",
  "main": "./build/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "postinstall": "tsc",
